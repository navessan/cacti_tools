<interface>
	<name>Get Trassir Archive HDD Stats</name>
	<script_path>|path_cacti|/scripts/ss_trassir_indexed.php</script_path>
	<script_function>ss_trassir_indexed</script_function>
	<script_server>php</script_server>
	<arg_prepend>https:|host_hostname|:port:user:password archive</arg_prepend>
	<arg_index>index</arg_index>
	<arg_query>query</arg_query>
	<arg_get>get</arg_get>
	<output_delimeter>:</output_delimeter>
	<index_order>Index</index_order>
	<index_order_type>numeric</index_order_type>
	<index_title_format>|chosen_order_field|</index_title_format>

	<fields>
		<Index>
			<name>Index</name>
			<direction>input</direction>
			<query_name>subdir_guid</query_name>
		</Index>	
		<serial>
			<name>serial</name>
			<direction>input</direction>
			<query_name>serial</query_name>
		</serial>
		<disk_id>
			<name>disk_id</name>
			<direction>input</direction>
			<query_name>disk_id</query_name>
		</disk_id>
		<model>
			<name>model</name>
			<direction>input</direction>
			<query_name>model</query_name>
		</model>
		<capacity_gb>
			<name>capacity_gb</name>
			<direction>output</direction>
			<query_name>capacity_gb</query_name>
		</capacity_gb>
		<archive_size_days>
			<name>archive_size_days</name>
			<direction>output</direction>
			<query_name>stats/archive_size_days</query_name>
		</archive_size_days>	
		<sync_mode>
			<name>sync_mode</name>
			<direction>output</direction>
			<query_name>stats/sync_mode</query_name>
		</sync_mode>		
		<in_use_by_db>
			<name>in_use_by_db</name>
			<direction>output</direction>
			<query_name>stats/in_use_by_db</query_name>
		</in_use_by_db>		
		<disk_write_mbs>
			<name>disk_write_mbs</name>
			<direction>output</direction>
			<query_name>stats/disk_write_mbs</query_name>
		</disk_write_mbs>		
		<last_error_code>
			<name>last_error_code</name>
			<direction>output</direction>
			<query_name>stats/last_error_code</query_name>
		</last_error_code>		
		<disk_read_mbs>
			<name>disk_read_mbs</name>
			<direction>output</direction>
			<query_name>stats/disk_read_mbs</query_name>
		</disk_read_mbs>		
		<current_speed_mbs>
			<name>current_speed_mbs</name>
			<direction>output</direction>
			<query_name>stats/current_speed_mbs</query_name>
		</current_speed_mbs>	
		<unavailable>
			<name>unavailable</name>
			<direction>output</direction>
			<query_name>stats/unavailable</query_name>
		</unavailable>	
		<error_counter>
			<name>error_counter</name>
			<direction>output</direction>
			<query_name>stats/error_counter</query_name>
		</error_counter>			
	</fields>
</interface>
